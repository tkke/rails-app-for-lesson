<!-- 発展課題 -->
<div class="col-xs-8" id="users-content">
  <div class="list-group">
    <div class="list-group-item user-list">
      <h2>ユーザー一覧</h2>
      <% @users.each do |user| %>
        <div class="list-group-item">
        	<h4 class="user">
			      <span class="user-name">
			        <%= link_to user.email, "/users/#{user.id}" %>
			      </span>
			    </h4>
	      </div>
      <%end%>
    </div>
  </div>
</div>
